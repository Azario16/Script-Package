{
  "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "rootDir": "./src",
      "outDir": "dist/js",
      "strict": true,
      "sourceMap": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "lib": ["es2015", "es6", "dom", "dom.iterable", "ES2020"],
      "downlevelIteration": true,
      "experimentalDecorators": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
  }
  
}
