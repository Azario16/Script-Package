{
  "name": "Script Package",
  "version": "1.0.0",
  "description": "Сборник скриптов для быстрого доступа к ресурсам: ://.skyeng.ru/* , ://datsy.ru/, ://skyeng.autofaq.ai/, ://skyeng.omnidesk.ru/*",
  "manifest_version": 2,
  "icons": {
    "16": "script-pack.png",
    "48": "script-pack.png",
    "128": "script-pack.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "script-pack.png",
      "32": "script-pack.png",
      "128": "script-pack.png"
    }
  },
  "background": {
    "scripts": [
      "static/js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "static/js/main.js"
      ],
      "css": [
        "static/css/main.css"
      ],
      "matches": [
        "*://crm2.skyeng.ru/*",
        "*://app.slack.com/*",
        "*://app.extension-test.ru/*",
        "*://skyeng.autofaq.ai/*",
        "*://extension-test.ru/*",
        "*://api.extension-test.ru/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "static/css/main.css",
    "libs/*",
    "autofaq/*"
  ],
  "permissions": [
    "cookies",
    "*://*.skyeng.ru/*",
    "*://skyeng.omnidesk.ru/*",
    "*://datsy.ru/api/*",
    "*://kibana-logs.skyeng.link/",
    "storage",
    "notifications"
  ]
}