{
  "name": "Script Package",
  "version": "0.6.305",
  "description": "Сборник скриптов для быстрого доступа к ресурсам: ://.skyeng.ru/* , ://datsy.ru/, ://skyeng.autofaq.ai/, ://skyeng.omnidesk.ru/*",
  "manifest_version": 2,
  "icons": {
    "16": "script-pack.png",
    "48": "script-pack.png",
    "128": "script-pack.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "static/js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "static/js/main.js"
      ],
      "css": [
        "static/css/main.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "libs/*",
    "autofaq/*"
  ],
  "permissions": [
    "cookies",
    "*://*.skyeng.ru/*",
    "*://skyeng.omnidesk.ru/*",
    "*://datsy.ru/api/*",
    "*://kibana-logs.skyeng.link/",
    "storage",
    "notifications"
  ]
}